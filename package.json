{
  "name": "vektorprogrammet",
  "version": "1.0.0",
  "description": "This is the source code for vektorprogrammets new homepage.",
  "main": " ",
  "dependencies": {
    "bower": "^1.7.9"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.0.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-uglify": "^1.5.1",
    "gulp-plumber": "^1.0.1"
  },
  "scripts": {
    "test": "./bin/phpunit --configuration app/phpunit.xml.dist",
    "setup": "composer install && npm install && ./node_modules/.bin/bower install && ./node_modules/.bin/gulp build:dev && php app/console doctrine:schema:create --env=dev && php app/console doctrine:fixtures:load --env=dev -n",
    "start": "php app/console server:run --docroot=www & npm run watch",
    "watch": "./node_modules/.bin/gulp",
    "reload-db": "php app/console doctrine:fixtures:load --env=dev",
    "build": "./node_modules/.bin/gulp build:dev",
    "build:prod": "./node_modules/.bin/gulp build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vektorprogrammet/webpage.git"
  },
