{% if step == 3 and data.totalInterviewsCount != 0 %}
    <p><a href="{{ path('admissionadmin_show', {'status':'interviewed'}) }}">Intervjuet {{ data.interviewedAssistantsCount }}/{{ data.totalInterviewsCount }}</a></p>
{% elseif step > 3 %}
    <p><a href="{{ path('admissionadmin_show', {'status':'interviewed'}) }}">{{ data.totalInterviewsCount }} intervjuer</a></p>
{% endif %}
