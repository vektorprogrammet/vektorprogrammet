{% if assistantHistory is not empty %}
    <div class="card mt-3">
        <div class="card-body">
            <div class="card-title">
                <h3 class="text-dark-blue">Assistenthistorikk</h3>
            </div>
            <div class="table-responsive">
                <table class="table">
                    <thead class="thead-light">
                    <tr>
                        <th>Skole</th>
                        <th>Semester</th>
                    </tr>
                    </thead>
                    {% for as in assistantHistory %}
                        {% if as.semester is not null and as.school is not null %}
                            <tr>
                                <td>
                                    {% if as.school is null %}
                                        skolen har blitt slettet
                                    {% else %}
                                        {{ as.getSchool.getName }}
                                    {% endif %}
                                </td>
                                <td>
                                    {% if as.semester is null %}
                                        semesteret har blitt slettet
                                    {% else %}
                                        {{ as.getSemester.getName }}
                                    {% endif %}
                                </td>
                            </tr>
                        {% endif %}

                    {% endfor %}
                </table>
            </div>
        </div>
    </div>
{% endif %}
