<div class="modal-dialog" role="document">
    {{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Fordel intervju</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-3">
                    {{ form_label(form.interviewer, 'Intervjuer', {'label_attr': {'class': 'inline'}}) }}
                </div>
                <div class="col-9">
                    {{ form_errors(form.interviewer) }}
                    {{ form_widget(form.interviewer) }}
                </div>
            </div>
            <div class="row">
                <div class="col-3">
                    {{ form_label(form.interviewSchema, 'Skjema', {'label_attr': {'class': 'inline'}}) }}
                </div>
                <div class="col-9">
                    {{ form_errors(form.interviewSchema) }}
                    {{ form_widget(form.interviewSchema) }}
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Avbryt</button>
            <button type="submit" class="btn btn-primary">Fordel</button>
        </div>
    </div>
    {{ form_end(form) }}
</div>
