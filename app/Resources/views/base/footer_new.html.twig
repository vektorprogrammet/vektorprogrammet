            <div id="googleMap" style="width:250px;height:350px;"></div>

            <script>
                function myMap() {
                    var locations = [
                        [63.4195014,10.40195],
                        [60.3878612,5.3195662],
                        [59.6671086,10.765153],
                        [69.1898477,17.6760827],
                        [59.9399586,10.7195609]
                    ];

                    var mapProp= {
                        center:new google.maps.LatLng(65.1195014,10.40195),
                        zoom:4,
                    };

                    var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);

                    for(var i=0;i<locations.length;i++){
                        var marker = new google.maps.Marker({position: new google.maps.LatLng(locations[i][0],
                            locations[i][1]),
                            map: map});
                        marker.setMap(map);
                    }
                }
            </script>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAyesbQMyKVVbBgKVi2g6VX7mop2z96jBo&callback=myMap"></script>
