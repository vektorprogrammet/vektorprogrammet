<a href="{{ path('home') }}">
    {% if "now"|date("m") == "12" %}
        <img src="{{ asset_with_version('images/vektor-christmas-logo.png') }}" class="logo img-fluid" alt="Vektor logo">
    {% else %}
        <img src="{{ asset_with_version('images/vektor-logo.svg') }}" class="logo img-fluid" alt="Vektor logo">
    {% endif %}
</a>
