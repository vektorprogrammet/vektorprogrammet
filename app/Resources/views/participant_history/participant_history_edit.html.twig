{% extends 'adminBase.html.twig' %}

{% block title %}
    Rediger assistenthistorie
{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-12 col-md-12 col-lg-12 columns">

            <h1> Rediger assistenthistorie </h1>

            <hr>

            <div class="row">
                <div class="col-12 col-md-12 col-lg-12 columns">

                    {{ form_start(form) }}
                    {{ form_row(form.Semester) }}
                    {{ form_row(form.workdays) }}
                    {{ form_row(form.School) }}
                    {{ form_row(form.bolk) }}
                    {{ form_row(form.day) }}
                    {{ form_row(form.save, {'label': 'Lagre'}) }}
                    {{ form_end(form) }}

                </div>
            </div>


        </div>
    </div>
{% endblock %}
