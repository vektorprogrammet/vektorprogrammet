<!DOCTYPE html>
<html lang="no">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{% block title %}Vektorprogrammet{% endblock %}</title>

    <!-- Facebook meta data -->
    <meta property="og:type" content="website"/>
    {% if app.request.attributes.get('_route') is not null %}
        <meta property="og:url" content="http://vektorprogrammet.no/{{ path(app.request.attributes.get('_route'),
        app.request.attributes.get('_route_params')) }}"/>
    {% endif %}
    <meta property="og:title" content="{{ block('title') }}"/>
    <meta property="og:image"
          content="http://vektorprogrammet.no{% block facebookImage %}{{ asset_with_version('images/vektor_stor.png') }}{% endblock %}"/>
    <meta property="og:description"
          content="{% block facebookDescription %}Vektorprogrammet er Norges største organisasjon som jobber for å øke interessen for matematikk og realfag blant elever i grunnskolen. Vi sender realfagssterke studenter til barne- og ungdomsskoler hvor de fungerer som lærerens assistent.{% endblock %}"/>
    <meta property="og:site_name" content="Vektorprogrammet"/>

    <!-- Color for header in mobile browsers -->
    <meta name="theme-color" content="#6FCEEE"><!-- Chrome, Firefox OS and Opera -->
    <meta name="msapplication-navbutton-color" content="#6FCEEE"><!-- Windows Phone -->
    <meta name="apple-mobile-web-app-status-bar-style" content="#6FCEEE"><!-- iOS Safari -->

    {#<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">#}
    {#<link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.blue-red.min.css" />#}

    {#<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">#}
    {#<link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">#}
    {#<script defer src="{{ asset('js/material.js') }}"></script>#}

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
    {#<link rel="stylesheet" href="{{ asset_with_version('css/getmdl-select.min.css') }}">#}
    <link rel="stylesheet" href="{{ asset_with_version('css/material-components-web.min.css') }}">
    <link rel="stylesheet" href="{{ asset_with_version('css/flexboxgrid.min.css') }}">
    <link rel="stylesheet" href="{{ asset_with_version('css/app_new.css') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset_with_version('favicon.ico') }}"/>
    {% block stylesheets %}{% endblock %}

    {% block customStyles %}{% endblock %}

    <script type="text/javascript">
      window.smartlook || (function (d) {
        var o = smartlook = function () {
          o.api.push(arguments)
        }, h = d.getElementsByTagName('head')[0];
        var c = d.createElement('script');
        o.api = new Array();
        c.async = true;
        c.type = 'text/javascript';
        c.charset = 'utf-8';
        c.src = 'https://rec.smartlook.com/recorder.js';
        h.appendChild(c);
      })(document);
      smartlook('init', '{{ smartlookID }}');
    </script>

</head>
<body>
<div class="page">
    {% block header %}
        {% include 'base/header_new.html.twig' %}
    {% endblock %}
        <div class="">

            {% block body %}
                <p>
                    Content
                </p>
            {% endblock %}
        </div>

        {% block footer %}
            {% include 'base/footer_new.html.twig' %}
        {% endblock %}
    </div>

<script>
  (function (i, s, o, g, r, a, m) {
    i['GoogleAnalyticsObject'] = r;
    i[r] = i[r] || function () {
          (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date();
    a = s.createElement(o),
        m = s.getElementsByTagName(o)[0];
    a.async = 1;
    a.src = g;
    m.parentNode.insertBefore(a, m)
  })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

  ga('create', '{{ googleAnalyticsId }}', 'auto');
  ga('send', 'pageview');

</script>

{#<script src="{{ asset_with_version('js/material-components-web.min.js') }}"></script>#}
{#<script>mdc.autoInit()</script>#}
<script src="{{ asset_with_version('js/app_new.js') }}"></script>
{#<script src="{{ asset_with_version('js/getmdl-select.min.js') }}"></script>#}
{% block javascripts %}{% endblock %}

</body>
</html>
