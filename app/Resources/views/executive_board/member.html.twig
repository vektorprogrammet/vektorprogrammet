{% extends 'adminBase.html.twig' %}

{% block title %}
    Hovedstyret
{% endblock %}

{% block breadcrumb %}
    <li class="breadcrumb-item">
        <a href="{{ path('executive_board_show') }}">Hovedstyret</a>
    </li>
    <li class="breadcrumb-item active">Medlem</li>
{% endblock %}


{% block body %}
    <div class="row">
        <div class="col-12 col-md-12 col-lg-6">
            <div class="card">
                <div class="card-header"><i class="fa fa-user-o"></i> {{ heading }}</div>
                <div class="card-body">
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <button class="btn btn-primary" type="submit">Lagre</button>
                    {{ form_end(form) }}

                </div>

            </div>


        </div>
    </div>

{% endblock %}
