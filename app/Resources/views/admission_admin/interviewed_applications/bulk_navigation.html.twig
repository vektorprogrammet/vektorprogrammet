<div class="row">
        <div class="col-12 col-md-8 col-lg-6 columns">
            <div class="row collapse">
                {% if is_granted_admin() %}
                <div class="col-4 columns">
                    <select id="bulkSelect">
                        <option value="title">Gruppehandling</option>
                        <option value="deleteApplication">Slett søknad</option>
                        <option value="deleteInterview">Slett intervju</option>
                    </select>
                </div>
                <div class="col-2 columns">
                    <a href="#" id="bulkAction" class="button postfix">Utfør</a>
                </div>
                {% endif %}
                {% if is_granted_team_leader() and department == semester.department %}
                    <div class="col-3 columns"style="padding-left: 15px">
                        <a class="button tiny" href="{{ path('admissionadmin_team_interest_by_semester' , { 'semester': semester.id })}}">Team-interesse</a>
                    </div>
                {% endif %}
            </div>
        </div>
    <div class="col-12 {{ is_granted_admin() ? 'col-md-4 col-lg-6' }} columns">
            <span class="float-right">Ja: {{ yes }} / Kanskje: {{ maybe }} / Nei: {{ no }} /
                Totalt: {{ applications | length }}</span>
    </div>
</div>
