{%- block choice_widget_expanded -%}
    <ul {{ block('widget_container_attributes') }} class="no-bullet alternatives">
        {%- for child in form %}
            <li>
                {{- form_widget(child) -}}
                {{- form_label(child) -}}
            </li>
        {% endfor -%}
    </ul>
{%- endblock choice_widget_expanded -%}
